{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "nome",
        "preco",
        "descricao",
        "quantidade",
        "_id"
    ],
    "additionalProperties": true,
    "properties": {
        "nome": {
            "type": "string"
        },
        "preco": {
            "type": "integer"
        },
        "descricao": {
            "type": "string"
        },
        "quantidade": {
            "type": "integer"
        },
        "_id": {
            "type": "string"
        }
    }
}